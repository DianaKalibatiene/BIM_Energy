publisher = {MDPI AG},
title = {{An IFC interoperability framework for self-inspection process in buildings}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85042473786{\&}doi=10.3390{\%}2Fbuildings8020032{\&}partnerID=40{\&}md5=7bfc1a58ec0f1dee38fcd505ba49ef05},
