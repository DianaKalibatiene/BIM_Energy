title = {{Thermal building simulation based on BIM-models [Thermisch-energetische Geb{\"{a}}udesimulation auf Basis eines Bauwerksinformationsmodells]}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85045418924{\&}doi=10.1002{\%}2Fbapi.201810008{\&}partnerID=40{\&}md5=39f695b41492cbb74e6a711b2c4a2af0},
