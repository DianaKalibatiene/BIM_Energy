journal = {Journal of Cleaner Production},
keywords = { Architectural design, Building Information Modelling; Circular economy; Design for deconstruction; Disassembly and deconstruction analytics (D-DAS); End of lives,Construction; Decision support systems; Digital storage; Ecodesign; Information theory; Life cycle; Semantics; Sustainable development},
pages = {386--396},
publisher = {Elsevier Ltd},
title = {{Disassembly and deconstruction analytics system (D-DAS) for construction in a circular economy}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85063271544{\&}doi=10.1016{\%}2Fj.jclepro.2019.03.172{\&}partnerID=40{\&}md5=3051745afc9b10d2b5706c5ddfaaf3a1},
