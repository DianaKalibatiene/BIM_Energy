journal = {Journal of Cleaner Production},
keywords = { Bio-based; Bio-based materials; Bosnia and herzegovina; Eco-friendly construction; Energy analysis; Energy use intensities; Environmental performance; Global warming potential, Heating,Architectural design; Concrete buildings; Concrete construction; Concretes; Construction industry; Cost benefit analysis; Costs; Energy efficiency; Energy management; Energy utilization; Environmental impact; Environmental management; Global warming; Houses; Housing; Regulatory compliance},
pages = {968--976},
publisher = {Elsevier Ltd},
