publisher = {Elsevier Ltd},
title = {{A new method for calculating the embodied carbon emissions from buildings in schematic design: Taking building element as basic unit}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85092520469{\&}doi=10.1016{\%}2Fj.buildenv.2020.107306{\&}partnerID=40{\&}md5=10ddb82e8af24b70637387b9fa95d420},
