title = {{Recommendations for developing a BIM for the purpose of LCA in green building certifications}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85089388649{\&}doi=10.3390{\%}2Fsu12156151{\&}partnerID=40{\&}md5=9e3cec895af1bf7da3540cd3d7deefb7},
volume = {12},
