journal = {Engineering Applications of Artificial Intelligence},
keywords = { Agent-based approach; Agent-based model; Building design; Building design process; Building Information Modelling; Occupant's behaviours; Residential dwelling; Standard exchange format, Architectural design,Air quality; Autonomous agents; Computational methods; Energy utilization; Housing; Information theory; Simulation platform; Software reliability; Waste management},
pages = {44--59},
publisher = {Elsevier Ltd},
title = {{Li-BIM, an agent-based approach to simulate occupant-building interaction from the Building-Information Modelling}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85063447676{\&}doi=10.1016{\%}2Fj.engappai.2019.03.008{\&}partnerID=40{\&}md5=4b9868c8850969ace7695abbd72181f0},
