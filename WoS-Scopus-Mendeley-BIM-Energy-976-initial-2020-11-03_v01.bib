publisher = {Elsevier Ltd},
title = {{A catalogue of energy conservation measures (ECM) and a tool for their application in energy simulation models}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85076241709{\&}doi=10.1016{\%}2Fj.jobe.2019.101102{\&}partnerID=40{\&}md5=0bfe1cca60249fd9758da62ff066c52c},
