keywords = { Architectural design, Building energy use; Building Information Model - BIM; Building life cycle; Energy efficient building; High temporal resolution; Integrated frameworks; Life Cycle Assessment (LCA); Straight-forward method,Climate change; Ecodesign; Electric power transmission networks; Energy efficiency; Energy utilization; Gas emissions; Greenhouse gases; Intelligent buildings; Life cycle; Office buildings; Structural design; Weather forecasting},
publisher = {Elsevier Ltd},
title = {{Exploring the effects that a non-stationary climate and dynamic electricity grid mix has on whole building life cycle assessment: A multi-city comparison}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85086594982{\&}doi=10.1016{\%}2Fj.scs.2020.102294{\&}partnerID=40{\&}md5=833e77bf3e4e9cb398cc1e6140569fdc},
