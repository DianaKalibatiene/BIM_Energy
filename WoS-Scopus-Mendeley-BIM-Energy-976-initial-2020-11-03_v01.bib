pages = {385--396},
publisher = {Elsevier Ltd},
title = {{A new systemic approach to improve the sustainability performance of office buildings in the early design stage}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84969793945{\&}doi=10.1016{\%}2Fj.enbuild.2015.09.040{\&}partnerID=40{\&}md5=ee4f02f3adc5440a56ffe5608bca4059},
volume = {109},
