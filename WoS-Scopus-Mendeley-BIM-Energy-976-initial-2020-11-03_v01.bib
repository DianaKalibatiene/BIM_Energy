@article{Ustinovichius20181136,
abstract = {A conceptual model of BIM-based design and refurbishment, based on pre-built indicators and allowing the assessment of the building energy demand and eco-building parameters, is presented. The new approach presented in this model creates a knowledge-based decision-making environment for refurbishment strategies and quality control, in this way creating the preconditions to bridge the gap between expected and actual energy performance. The model with integration of new BIM-based optimization subsystems enables energy management and optimization processes. For a comprehensive evaluation of refurbishment measures, it is suggested to include energy efficiency, eco-efficiency, and economic parameters. {\textcopyright} 2018 Politechnika Wroc{\l}awska},
