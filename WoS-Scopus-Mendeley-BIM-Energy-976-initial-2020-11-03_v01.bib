journal = {BUILDING SIMULATION},
number = {4},
pages = {739--751},
publisher = {TSINGHUA UNIV PRESS},
title = {{A review of operational energy consumption calculation method for urban buildings}},
type = {Review},
volume = {13},
year = {2020}
}
