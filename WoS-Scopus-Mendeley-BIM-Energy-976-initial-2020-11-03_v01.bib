keywords = { Building Information Modelling; Construction planning; Design management; Design/methodology/approach; Environmental awareness; Leadership in energy and environmental designs; Organizational constraints; Structural equation modeling, Structural design,Architectural design; Environmental design; Image enhancement; Personnel},
number = {1},
pages = {29--45},
publisher = {Emerald Group Publishing Ltd.},
title = {{Environmental design and awareness impact on organization image}},
