keywords = { Building energy use; Building sectors; Different climatic zones; Learning-based algorithms; Life cycle energies; Operating energies; Reduction measures; Systematic Review, Structural design,Buildings; Deep learning; Economic and social effects; Evolutionary algorithms; Gas emissions; Greenhouse gases; Learning algorithms; Life cycle},
publisher = {Elsevier Ltd},
title = {{Evaluating the impact of operating energy reduction measures on embodied energy}},
