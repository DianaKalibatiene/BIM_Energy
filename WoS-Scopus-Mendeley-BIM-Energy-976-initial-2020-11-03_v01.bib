keywords = { Architectural design, Building design; Building energy efficiency; Cost assessment; Early decision; Early designs; Early energy; Healthcare buildings; Zero emission buildings,Air pollution; Carbon dioxide; Cost estimating; Decision making; Decision support systems; Energy conservation; Energy efficiency; Energy utilization; Global warming; Health care; Software design; Structural design; Zero energy buildings},
number = {7},
publisher = {MDPI AG},
title = {{An assisted workflow for the early design of nearly zero emission healthcare buildings}},
