title = {{An efficient global energy optimization approach for robust 3D plane segmentation of point clouds}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85041388663{\&}doi=10.1016{\%}2Fj.isprsjprs.2018.01.013{\&}partnerID=40{\&}md5=3a51448902248e5ca7e81c99452a6f95},
