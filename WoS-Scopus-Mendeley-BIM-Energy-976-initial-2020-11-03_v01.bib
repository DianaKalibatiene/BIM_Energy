pages = {138--157},
title = {{Building information modelling (BIM) for sustainable building design}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84874417652{\&}doi=10.1108{\%}2F02632771311299412{\&}partnerID=40{\&}md5=5198540ace68d90fffde6f17f213a4cf},
