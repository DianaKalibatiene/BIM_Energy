publisher = {Emerald Group Publishing Ltd.},
title = {{A systematic review of BIM usage for life cycle impact assessment}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85086785237{\&}doi=10.1108{\%}2FBEPAM-03-2019-0028{\&}partnerID=40{\&}md5=5a42a23bd75c13b4e1ad12e765379541},
volume = {10},
