pages = {634--649},
publisher = {Elsevier Ltd},
title = {{Optimizing the energy consumption in a residential building at different climate zones: Towards sustainable decision making}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85067357799{\&}doi=10.1016{\%}2Fj.jclepro.2019.06.093{\&}partnerID=40{\&}md5=1e1e57addc52b1cf604d8aeea8e0b28d},
volume = {233},
