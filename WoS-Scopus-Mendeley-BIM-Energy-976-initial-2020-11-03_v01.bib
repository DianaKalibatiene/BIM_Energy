publisher = {Elsevier B.V.},
title = {{BIM IFC information mapping to building energy analysis (BEA) model with manually extended material information}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84967016888{\&}doi=10.1016{\%}2Fj.autcon.2016.04.002{\&}partnerID=40{\&}md5=ee50dd23c56ffa1cf33c97ce15dfdac3},
