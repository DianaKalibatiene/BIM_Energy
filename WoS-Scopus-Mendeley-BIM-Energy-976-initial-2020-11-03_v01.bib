journal = {Journal of Information Technology in Construction},
keywords = { Architectural design, Architecture , engineering and constructions; Building life cycle; Building life cycle management; Cloud computing technologies; Construction sectors; Construction stages; Empirical research; Facility management,Buildings; Cloud computing; Construction; Construction industry; Energy efficiency; Engineering research; Information management; Life cycle; Office buildings; Stages},
pages = {281--291},
publisher = {Department of Computer Science},
title = {{A review of cloud-based bim technology in the construction sector}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84969951849{\&}partnerID=40{\&}md5=cf278d6342028706c00a2bbfae9fd88c},
volume = {19},
year = {2014}
