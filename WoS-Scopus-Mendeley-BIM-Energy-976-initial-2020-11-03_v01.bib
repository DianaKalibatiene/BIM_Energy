issn = {23524847},
journal = {Energy Reports},
keywords = { Architectural design, Building Information Model - BIM; CFD analysis; Computational fluid dynamics modeling; Design process; Natural ventilation; Operational characteristics; Physical specifications; Three-dimensional (3D) buildings,3D modeling; Air; Air conditioning; Computational fluid dynamics; Cooling; Energy utilization; Structural design; Thermal comfort; Ventilation},
pages = {644--661},
publisher = {Elsevier Ltd},
title = {{An evaluation process for natural ventilation using a scenario-based multi-criteria and multi-interaction analysis}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85081116984{\&}doi=10.1016{\%}2Fj.egyr.2020.02.001{\&}partnerID=40{\&}md5=774ee4cdd8bde3927c6a254be2f88392},
