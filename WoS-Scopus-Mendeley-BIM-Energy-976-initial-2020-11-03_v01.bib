author = {Yang, X and Hu, M and Wu, J and Zhao, B},
doi = {10.1016/j.jclepro.2018.02.070},
issn = {09596526},
journal = {Journal of Cleaner Production},
keywords = { Architectural design, Architecture , engineering and constructions; Building Information Model - BIM; Construction machines; Construction technique; Environmental performance; Life Cycle Assessment (LCA); Life Cycle Inventory; Low-carbon designs,Building materials; Buildings; Carbon footprint; Computer aided software engineering; Computer software; Construction; Data flow analysis; Database systems; Ecodesign; Energy conservation; Energy utilization; Environmental impact; Environmental management; Greenhouse gases; Housing; Information theory; Life cycle},
