pages = {1199--1209},
publisher = {Elsevier Ltd},
title = {{The impact of energy audits on energy efficiency investment of public owners. Evidence from Italy}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84949680006{\&}doi=10.1016{\%}2Fj.energy.2015.09.117{\&}partnerID=40{\&}md5=8c465179c4b09dda3b338cd077e1c913},
