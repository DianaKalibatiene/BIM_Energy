journal = {Journal of Materials Chemistry A},
keywords = { Anode material for lithium ion batteries; High current densities; High reversible capacities; Large specific surface areas; Metal oxide composite; Metalorganic frameworks (MOFs); Supercapacitor electrodes; Two Dimensional (2 D), Storage (materials),Anodes; Costs; Crystalline materials; Doping (additives); Electrodes; Electron emission; Energy storage; Lithium-ion batteries; Metals; Nanosheets; Organometallics},
number = {5},
pages = {2166--2175},
