journal = {Annals of GIS},
keywords = { India,energy conservation; GIS; nonrenewable resource; photovoltaic system; software; thermal power; urban planning},
number = {1},
pages = {27--41},
publisher = {Taylor and Francis Ltd.},
title = {{CityGML at semantic level for urban energy conservation strategies}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84926190849{\&}doi=10.1080{\%}2F19475683.2014.992370{\&}partnerID=40{\&}md5=165e077c500ff4e65ef4afee73885455},
volume = {21},
