issn = {09242716},
journal = {ISPRS Journal of Photogrammetry and Remote Sensing},
keywords = { Building Information Model - BIM; Energy optimization; Multiscale supervoxel; Plane segmentation; Point cloud registration; Region growing; Region growing algorithm; Simultaneous localization and mapping, Three dimensional computer graphics, algorithm; benchmarking; data quality; data set; energy efficiency; experimental study; numerical model; optimization; precision; sampling; segmentation; simulated annealing,Architectural design; Image segmentation; Robotics; Simulated annealing},
pages = {112--133},
