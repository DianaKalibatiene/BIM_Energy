publisher = {Elsevier Ltd},
title = {{Multicomponent energy assessment of buildings using building information modeling}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85067472252{\&}doi=10.1016{\%}2Fj.scs.2019.101603{\&}partnerID=40{\&}md5=e7eccf225562033c3ded0c8270cabca5},
