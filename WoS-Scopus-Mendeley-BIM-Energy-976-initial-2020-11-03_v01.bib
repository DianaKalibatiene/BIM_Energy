author = {Spiegelhalter, T},
doi = {10.18848/2325-1662/cgp/v06i02/38326},
issn = {23251662},
journal = {International Journal of Architectonic, Spatial, and Environmental Design},
number = {2},
pages = {27--41},
publisher = {Common Ground Research Networks},
title = {{Designing and manufacturing zero fossil energy buildings with 3D/4D parametric performance tools}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84900453635{\&}doi=10.18848{\%}2F2325-1662{\%}2Fcgp{\%}2Fv06i02{\%}2F38326{\&}partnerID=40{\&}md5=ce181a855da57a4739f024fecf76d5ec},
