publisher = {Hindawi Publishing Corporation},
title = {{Solving Civil Engineering Problems by Means of Fuzzy and Stochastic MCDM Methods: Current State and Future Research}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84944198696{\&}doi=10.1155{\%}2F2015{\%}2F362579{\&}partnerID=40{\&}md5=4fe1cdb6e9aeab422e6c8ec583d535ea},
