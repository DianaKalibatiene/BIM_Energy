keywords = { Architectural design; Housing; Life cycle; Tools, Carbon,BIM; Building Information Model - BIM; Construction method; Current construction; Embodied carbons; Life Cycle Assessment (LCA); Structural characteristics; Thermal characteristics},
pages = {479--488},
title = {{Embodied and operational energy for new-build housing: A case study of construction methods in the UK}},
