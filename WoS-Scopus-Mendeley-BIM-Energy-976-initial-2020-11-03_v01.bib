journal = {Computer Science - Research and Development},
keywords = { Building Information Model - BIM; Digital counterparts; Domain specific semantics; Geometric representation; Industrial foundation class; Industry Foundation Classes - IFC; Semantic constraints; Simplification method, Energy efficiency,Architectural design; Building materials; Computer aided design; Construction industry; Doors; Functional groups; Geometry; Information filtering; Information theory; Interoperability; Office buildings; Semantics},
number = {3},
pages = {119--125},
publisher = {Springer Verlag},
title = {{Geometry simplification according to semantic constraints: Enabling energy analysis based on building information models}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84913580558{\&}doi=10.1007{\%}2Fs00450-014-0283-7{\&}partnerID=40{\&}md5=32c98c44fc921007f3aabfad3d11ac9b},
