publisher = {Elsevier Ltd},
title = {{Case-based reasoning approach for supporting building green retrofit decisions}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85067477540{\&}doi=10.1016{\%}2Fj.buildenv.2019.106210{\&}partnerID=40{\&}md5=a1f85c7442c25af165526fc24235b3d9},
