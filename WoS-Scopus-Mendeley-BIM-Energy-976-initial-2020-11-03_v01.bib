keywords = { Building Information Model - BIM; Life Cycle Assessment (LCA); Operating energy consumption; Operational energy; Performance aspects; Sustainability assessment; Triple Bottom Line; Window-to-wall ratio, Environmental impact, United States, building; energy use; environmental impact assessment; life cycle analysis; sustainability,Architectural design; Ecodesign; Energy conservation; Energy utilization; HVAC; Life cycle; Office buildings; Sustainable development; Walls (structural partitions)},
publisher = {Elsevier Ltd},
title = {{Triple bottom line sustainability assessment of window-to-wall ratio in US office buildings}},
