journal = {International Journal of Sustainable Building Technology and Urban Development},
keywords = {comparative study; construction material; energy efficiency; environmental impact; environmental management; global warming; health impact; life cycle analysis},
number = {1},
pages = {14--24},
publisher = {Taylor and Francis Ltd.},
title = {{Life cycle environmental performance of material specification: a BIM-enhanced comparative assessment}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84946575062{\&}doi=10.1080{\%}2F2093761X.2015.1006708{\&}partnerID=40{\&}md5=46a3d5e8db1d6dcc5995175aee4f8e69},
