keywords = { Application program interfaces; Building Information Model - BIM; Certification systems; Environmentally friendly buildings; Green buildings; Integrated sustainable designs; K nearest neighbours (k-NN); Sustainable cities, Architectural design,Application programming interfaces (API); Application programs; Construction; Construction industry; Data mining; Ecodesign; Environmental design; Nearest neighbor search; Sustainable development},
publisher = {Elsevier Ltd},
title = {{An integrated BIM-LEED application to automate sustainable design assessment framework at the conceptual stage of building projects}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85075721278{\&}doi=10.1016{\%}2Fj.scs.2019.101979{\&}partnerID=40{\&}md5=81595d46cffee25b7eb1ebda3d8bcb24},
volume = {53},
year = {2020}
}
@article{Chihib2020,
abstract = {Several factors impact the energy use of university campus buildings. This study aims to benchmark the energy use in universities with Mediterranean climates. The University of Almeria campus was used as a case study, and different types of buildings were analyzed. The second goal was to model the electricity consumption and determinate which parameter correlate strongly with energy use. Macro-scale energy consumption data during a period of seven years were gathered alongside cross-sectional buildings information. Eight years of daily outdoor temperature data were recorded and stored for every half hour. This dataset was eventually used to calculate heating and cooling degree-days. The weather factor was recognized as the variable with the greatest impact on campus energy consumption, and as the coefficient indicated a strong correlation, a linear regression model was established to forecast future energy use. A threshold of 8 GWh has been estimated as the energy consumption limit to be achieved despite the growth of the university. Finally, it is based on the results to inform the recommendations for decision making in order to act effectively to optimize and achieve a return on investment. {\textcopyright} 2020 by the authors.},
