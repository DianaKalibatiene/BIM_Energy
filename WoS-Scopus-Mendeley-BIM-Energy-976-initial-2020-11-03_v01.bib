publisher = {Elsevier B.V.},
title = {{BIM-oriented data mining for thermal performance of prefabricated buildings}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85062385791{\&}doi=10.1016{\%}2Fj.ecoinf.2019.02.012{\&}partnerID=40{\&}md5=a4544c5b2a7db7d696e803c4f6b427f4},
