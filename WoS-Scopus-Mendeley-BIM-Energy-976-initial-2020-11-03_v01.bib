doi = {10.36680/j.itcon.2020.013},
issn = {18744753},
journal = {Journal of Information Technology in Construction},
keywords = { Architectural design, Building dimensions; Decision making process; Early design stages; Efficiency calculations; Energy consumption and cost; Information requirement; Life cycle energies; Performance criterion,Curricula; Decision making; Energy efficiency; Energy utilization; Graphic methods; Iterative methods; Life cycle; Structural design},
pages = {212--232},
publisher = {International Council for Research and Innovation in Building and Construction},
title = {{Consistent management and evaluation of building models in the early design stages}},
