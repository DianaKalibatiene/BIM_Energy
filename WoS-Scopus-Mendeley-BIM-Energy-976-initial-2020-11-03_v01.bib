pages = {256--276},
publisher = {Elsevier B.V.},
title = {{Historic Building Information Modelling: performance assessment for diagnosis-aided information modelling and management}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85037354812{\&}doi=10.1016{\%}2Fj.autcon.2017.11.009{\&}partnerID=40{\&}md5=e9f543f62fd76e3555555186a3e10870},
