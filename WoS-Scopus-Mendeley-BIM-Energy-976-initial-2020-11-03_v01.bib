issn = {15526100},
journal = {Journal of Green Building},
number = {2},
pages = {74--86},
publisher = {College Publishing},
title = {{Achieving the net-zero-energy-buildings "2020 and 2030 Targets" with the support of parametric 3-D and 4-D bim design tools}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84865396607{\&}doi=10.3992{\%}2Fjgb.7.2.74{\&}partnerID=40{\&}md5=58fad59b69558527d8c41d6950bb5812},
