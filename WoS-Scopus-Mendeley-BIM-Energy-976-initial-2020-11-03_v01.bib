number = {C},
pages = {16--28},
publisher = {Elsevier},
title = {{Developing a physical BIM library for building thermal energy simulation}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84926189988{\&}doi=10.1016{\%}2Fj.autcon.2014.10.011{\&}partnerID=40{\&}md5=e3f728f8db549bc9c68f420ed6f4b882},
