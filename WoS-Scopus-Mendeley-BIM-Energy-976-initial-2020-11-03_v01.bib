keywords = { Building Information Model - BIM; Cognitive workloads; Original time; Payment; Rework time; Technology innovation; Three-dimensional (3D) image; Two Dimensional (2 D), Visualization,Architectural design; Augmented reality; Costs; Errors},
number = {1},
publisher = {American Society of Civil Engineers (ASCE)},
title = {{Using augmented reality to facilitate piping assembly: An experiment-based evaluation}},
