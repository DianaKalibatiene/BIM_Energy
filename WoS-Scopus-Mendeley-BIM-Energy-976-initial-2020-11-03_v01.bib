publisher = {MDPI AG},
title = {{Automatically creating HVAC control strategies based on building information modeling (BIM): Heat provisioning and distribution}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85090906260{\&}doi=10.3390{\%}2Fen13174403{\&}partnerID=40{\&}md5=577fb8248c0664f21a56a14da22de30d},
