publisher = {Elsevier Ltd},
title = {{Sustainability-led design: Feasibility of incorporating whole-life cycle energy assessment into BIM for refurbishment projects}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85063520646{\&}doi=10.1016{\%}2Fj.jobe.2019.01.027{\&}partnerID=40{\&}md5=854b5a309bb882047b1892bfe6287119},
