doi = {10.3390/su11164398},
issn = {20711050},
journal = {Sustainability (Switzerland)},
keywords = { Scopus,architectural design; bibliography; civil engineering; database; demolition; design method; mapping method; science and technology; university sector},
number = {16},
publisher = {MDPI AG},
title = {{Bibliometric maps of BIM and BIM in universities: A comparative analysis}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85070746154{\&}doi=10.3390{\%}2Fsu11164398{\&}partnerID=40{\&}md5=4ce3107fd824f6ac3ee3289159057f01},
volume = {11},
year = {2019}
