@article{Gantner2018,
abstract = {Life cycle assessment (LCA) is the standard method for the quantification of environmental impacts within the construction sector, relying on available generic LCA databases. New developments, such as the increased influence of the building construction for LCA and the forthcoming of building information modeling (BIM), implicate new requirements on multiscale levels of development and complexity for LCA construction databases. At the example of the German "{\"{o}}kobau.dat", one of the leading LCA construction databases, this publication discusses whether the database is able to meet these requirements. The analysis shows the strengths of the {\"{o}}kobau.dat with regard to standardization conformity (EN 15804, ILCD), data provision in machine-readable XML format, and the provision of an application programming interface. Shortcomings include incorrect linking of building life cycle inventory data with environmental information, incorrect documentation of functional units, missing generic datasets, the modeling of energy use data or the lack of a uniform structuring, or material classification. The authors propose solutions such as the provision of appropriate functional units, the implementation of a top-down approach to investigate the completeness of data based on existing nomenclatures or the extension with an appropriate material classification. This would allow for future viability and adaptability of {\"{o}}kobau.dat for digital LCA. {\textcopyright} 2018 by the authors.},
annote = {cited By 7},
author = {Gantner, J and Lenz, K and Horn, R and von Both, P and Ebertsh{\"{a}}user, S},
doi = {10.3390/buildings8090129},
issn = {20755309},
journal = {Buildings},
number = {9},
publisher = {MDPI AG},
title = {{{\"{O}}kobau.dat 3.0-Quo vadis?}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85059584383{\&}doi=10.3390{\%}2Fbuildings8090129{\&}partnerID=40{\&}md5=ace72d956f400d1bfe513f4dcd034ed6},
volume = {8},
year = {2018}
}
@article{Thakur2018986,
abstract = {Sustainable building is one of the fastest growing concept which is seeding itself deep within the Architectural, Engineering and Construction industry. In the past few years there is a tremendous increase in the demand for the sustainable building because it help in conserving huge chunk of energy. G+3 office building situated in warm and humid climatic zone is used for this project purpose in order to demonstrate the use of BIM for sustainable building development. The goal of this study is to use eco-friendly materials in various building envelops of conventional building model and converting it into green building model using Autodesk Revit. The conventional building materials such as normal brick wall, RCC roof, marble flooring and single glazed windows are replaced by the eco-friendly materials like aerated blocks for walls, clay roof, cork flooring and double glazed clear glass with low E coating and the parameters such as performance factor of building, renewable energy potential, annual energy use/cost, monthly peak demand, monthly cooling and heating loads, annual carbon emission, and monthly electricity consumption checked for both the building models by using Autodesk green building studio. Both building models performance results were compared and it's seen how much efficient building has become by making the use of eco-friendly materials. {\textcopyright} 2018, Institute of Advanced Scientific Research, Inc. All rights reserved.},
