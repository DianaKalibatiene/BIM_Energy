keywords = { Architectural design, Building design; Building Information Modelling; Design features; Energy simulation; Hot climate; Scoring systems,Computer software; Cost benefit analysis; Energy efficiency; Energy utilization; Historic preservation; Sustainable development},
publisher = {Elsevier Ltd},
title = {{Quantitative approach for evaluating the building design features impact on cooling energy consumption in hot climates}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85078761677{\&}doi=10.1016{\%}2Fj.enbuild.2020.109802{\&}partnerID=40{\&}md5=0d196b528366099754aa8b27a0599d8a},
volume = {211},
