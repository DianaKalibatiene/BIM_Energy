publisher = {Emerald Group Publishing Ltd.},
title = {{BIM-based energy consumption assessment of the on-site construction of building structural systems}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85056748016{\&}doi=10.1108{\%}2FBEPAM-01-2018-0002{\&}partnerID=40{\&}md5=2e22de6d0add1e1128bf0ef5ae343ba4},
volume = {9},
