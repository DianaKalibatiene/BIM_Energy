journal = {INTERNATIONAL JOURNAL OF STRATEGIC PROPERTY MANAGEMENT},
keywords = {facility management; bibliometric-qualitative analysis; visual mapping; changing circumstances; facility manager; performance management},
number = {5},
pages = {354--365},
publisher = {VILNIUS GEDIMINAS TECH UNIV},
title = {{STATUS QUO AND FUTURE DIRECTIONS OF FACILITY MANAGEMENT: A BIBLIOMETRIC-QUALITATIVE ANALYSIS}},
type = {Article},
volume = {23},
year = {2019}
}
@article{ISI:000473514900032,
abstract = {At present, with the rapid development of information technology, people
have a much higher standard for the use function of various buildings
and their energy saving and environmental protection. Green intelligent
building, as a product of information society, is more and more popular.
This paper focuses on some advanced technologies such as BIM (Building
Information Modelling) to put forward the principles and specific
process of the green intelligent buildings automation system and its
system planning and design based on engineering practice after the full
research and comparative analysis of the basic concepts, basic
functions, current research status, development trend and system
structure concerning green intelligent building and its automation
system is done. It aims to provide overall solutions and ideas and
further enhance the competitiveness of products and markets of building
automation industry in China by combining professional theory with
engineering practice, thus laying the foundation for truly realising the
aims of `green' and `intelligent' of green intelligent buildings.},
