@article{Choi2015477,
abstract = {Building integrated photovoltaic (BIPV) systems are drawing attention as efficient energy generation systems because they combine functional building components with the capabilities of a photovoltaic system. It is important to verify the plant performance of BIPV systems according to building certification processes and mandatory requirements for new and renewable energy. However, a sufficient number of tests have not been conducted to set and verify policies for BIPV performance. Building information modeling (BIM) systems offer a major solution to the challenges associated with supplying, installing, and verifying BIPV systems. This study was conducted to promote the supply and stimulation of new and renewable energy sources. A BIM application plan is proposed to make the installation and supply of BIPV systems more efficient considering the certification system established by the government's new and renewable energy dissemination program. {\textcopyright} 2015, ICIC International.},
