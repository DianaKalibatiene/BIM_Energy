doi = {10.1016/j.jobe.2019.100775},
issn = {23527102},
journal = {Journal of Building Engineering},
keywords = { Artificial systems; Climate condition; Climate regions; Computational simulation; Energy efficient building; Environmental conditioning; Operational energy; Residential building, Ventilation,Architectural design; Cost benefit analysis; Energy efficiency; Environmental chambers; Intelligent buildings; Wind},
publisher = {Elsevier Ltd},
title = {{Operational energy of opaque ventilated fa{\c{c}}ades in Brazil}},
