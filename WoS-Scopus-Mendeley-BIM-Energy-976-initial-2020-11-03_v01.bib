author = {Dawood, N and Dawood, H and Rodriguez-Trejo, S and Crilly, M},
doi = {10.1186/s40327-017-0060-3},
issn = {22137459},
journal = {Visualization in Engineering},
number = {1},
publisher = {Springer},
title = {{Visualising urban energy use: the use of LiDAR and remote sensing data in urban energy planning}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85044635344{\&}doi=10.1186{\%}2Fs40327-017-0060-3{\&}partnerID=40{\&}md5=80b5c252ac018e6fd5fe7f6e2184cdad},
