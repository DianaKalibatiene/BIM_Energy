@article{Aguirre2020,
abstract = {In this work, the analysis of the mass transfer phenomena in catalytic open-cell foams is carried out through the combination of computational fluid dynamics (CFD) simulations and experiments, using the CO oxidation on Pt(1{\%})/$\gamma$-Al2O3/foam as a model reaction. The influence of the local hydrodynamic effects on the diffusion-reaction phenomena occurring at the gas-solid interface of the open-cell solid foams are investigated by Direct Numerical Simulations assuming an infinitely fast reaction. A correlation for the Sherwood number as function of Reynolds -for low Re- is proposed. To validate this experimentally, aluminum foams coated with Pt(1{\%})/$\gamma$-Al2O3 are tested at different reaction conditions for the CO oxidation. The obtained reaction rates and apparent activation energy show the presence of external mass transfer limitations. An analysis of the diffusion-reaction phenomena taking place in the wash-coated layer is presented in terms of dimensionless numbers. A practical criterion is developed in terms of the Thiele modulus (w) and the Biot number (Bim) for the identification of the reaction regimes: kinetic control (w2/Bim{\textless}0.1), internal and/or external mass transfer limitations (0.1{\textless}w2/Bim{\textless}10), and full mass transfer control (w2/Bim{\textgreater}10). {\textcopyright} 2020 The Authors},
