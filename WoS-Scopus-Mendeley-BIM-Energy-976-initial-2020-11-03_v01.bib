journal = {Energy and Buildings},
keywords = { Building energy consumption; Building energy consumption prediction; Electricity load forecasting; Generative Adversarial Nets; Machine learning models; Time series forecasting; Time series prediction; Traffic flow prediction, Energy conservation,Buildings; Deep learning; Electric power transmission networks; Energy management; Energy utilization; Forecasting; Heuristic methods; Image recognition; Speech processing; Speech recognition; Time series},
pages = {230--243},
publisher = {Elsevier Ltd},
title = {{Data Driven Parallel Prediction of Building Energy Consumption Using Generative Adversarial Nets}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85060874134{\&}doi=10.1016{\%}2Fj.enbuild.2019.01.034{\&}partnerID=40{\&}md5=5f4286f9d61b60c876f1b682e80a5bb2},
volume = {186},
