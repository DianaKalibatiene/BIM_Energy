keywords = {architectural design; cost analysis; decision making; economic analysis; energy conservation; energy efficiency; innovation; investment; life cycle analysis; maintenance; optimization},
number = {7},
publisher = {MDPI AG},
title = {{Development of A BIM-based maintenance decision-making framework for the optimization between energy efficiency and investment costs}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85050074793{\&}doi=10.3390{\%}2Fsu10072480{\&}partnerID=40{\&}md5=2cc9506c0fed0f6ce8a3d85d29f87df1},
volume = {10},
