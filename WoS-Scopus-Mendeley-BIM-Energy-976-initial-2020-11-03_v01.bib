issn = {15499596},
journal = {Journal of Chemical Information and Modeling},
keywords = { Anticancer treatment; Binding free energy; Cell-death pathways; Energy decomposition analysis; Molecular determinants; Principal Components; Structural differences; Structural feature, Binding energy,Cell death; Chemical activation; Free energy; Mammals; Molecular dynamics; Molecules; Peptides},
number = {3},
pages = {1632--1643},
publisher = {American Chemical Society},
title = {{Molecular Determinants for the Activation/Inhibition of Bak Protein by BH3 Peptides}},
