pages = {320--343},
publisher = {Elsevier Ltd},
title = {{Building information modelling based building energy modelling: A review}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85060224466{\&}doi=10.1016{\%}2Fj.apenergy.2019.01.032{\&}partnerID=40{\&}md5=037b1fe548ab9cac14682a9fb469f873},
