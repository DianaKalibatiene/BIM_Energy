issn = {20711050},
journal = {Sustainability (Switzerland)},
keywords = { United Kingdom,building; carbon emission; computer simulation; decision making; energy efficiency; information management; integrated approach; potential energy; social housing},
number = {9},
publisher = {MDPI AG},
title = {{Evaluating social housing retrofit options to support clients' decision Making-SIMPLER BIM protocol}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85066924059{\&}doi=10.3390{\%}2Fsu11092507{\&}partnerID=40{\&}md5=6a5332e48b328bb3aec35c43ed9017c7},
volume = {11},
