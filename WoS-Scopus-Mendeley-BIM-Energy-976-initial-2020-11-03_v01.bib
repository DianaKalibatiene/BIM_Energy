@article{Szer2020286,
abstract = {Buildings generate a significant amount of waste that has considerable impacts on environment and energy flow. This study aims to investigate the waste capacity of the selected district and its effect on the environment within the current waste management policy via Life Cycle Assessment (LCA) methodology based on the two indicators; global warming potential and energy flow. The district has 82 buildings with 64,971m2 total gross area and 2,000 populations. Accordingly, the district's waste capacity was evaluated within the classification of Municipal Solid Wastes (MSW), liquid wastes, and demolition wastes. The system boundaries of the LCA were set based on the gate-to-grave approach, which includes generated wastes during the lifespan of the buildings, including their end-of-life stage. Consequently, energy recovery potentials from waste processes were investigated and compared with the primary energy demand of the operational energy consumption of the buildings to perceive the amount of energy compensation range. Additionally, buildings' physical conditions, which obtained from their Building Information Models (BIM), energy performances, derived from their energy models and local specifications, obtained from standards were utilized to identify the current conditions and waste management systems. Critically, the outcomes of all those were used as input data for the LCA model. The results showed that there had been energy recovery potentials from MSW's treatments, while liquid wastes and demolishing wastes treatments have consumed energy. Energy recovery potential from MSW has compensated only 5.8{\%} of operational energy annually, which came from recycling processes. Also, all waste management systems release greenhouse gases to the atmosphere that cause global warming. {\textcopyright} 2020 The Authors},
annote = {cited By 3},
author = {S{\"{o}}zer, H and S{\"{o}}zen, H},
doi = {10.1016/j.egyr.2020.01.008},
issn = {23524847},
journal = {Energy Reports},
keywords = { Building Information Model - BIM; Demolition wastes; Environment and energies; Global warming potential; Life Cycle Assessment (LCA); Liquid wastes; Municipal solid waste (MSW); Waste management systems, Municipal solid waste,Architectural design; Demolition; Energy utilization; Global warming; Greenhouse gases; Information management; Life cycle; Liquids; Recovery; Waste management},
pages = {286--296},
publisher = {Elsevier Ltd},
title = {{Waste capacity and its environmental impact of a residential district during its life cycle}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85078256301{\&}doi=10.1016{\%}2Fj.egyr.2020.01.008{\&}partnerID=40{\&}md5=bf9af42ed39e861291e23d0743fb660a},
volume = {6},
