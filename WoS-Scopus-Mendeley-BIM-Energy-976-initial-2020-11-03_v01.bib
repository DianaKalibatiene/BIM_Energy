author = {Kim, S H},
doi = {10.3390/su10051319},
issn = {20711050},
journal = {Sustainability (Switzerland)},
keywords = { Korea,cooling; decision making; energy; heating; historic building; information technology; performance assessment; simulation; software},
number = {5},
publisher = {MDPI AG},
title = {{Assessing the needs and gaps of building information technologies for energy retrofit of historic buildings in the Korean context}},
