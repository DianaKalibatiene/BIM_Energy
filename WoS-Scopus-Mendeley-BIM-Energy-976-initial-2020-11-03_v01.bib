journal = {Advances in Building Energy Research},
keywords = { Building envelopes; Building Information Modelling; Energy efficient building; Energy modelling; Heat flux sensors; Heat gain reduction; Law of diminishing returns; United Arab Emirates, Energy efficiency,Architectural design; Climate change; Climate control; Heat flux; Historic preservation; Information theory; Infrared imaging; Intelligent buildings; Office buildings; Solar buildings; Thermography (imaging)},
publisher = {Taylor and Francis Ltd.},
title = {{The combined use of BIM, IR thermography and HFS for energy modelling of existing buildings and minimising heat gain through the building envelope: a case-study from a UAE building}},
