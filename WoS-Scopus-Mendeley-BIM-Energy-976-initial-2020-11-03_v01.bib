number = {4},
pages = {546--556},
publisher = {Taylor and Francis Inc.},
title = {{An open-source automated continuous condition-based maintenance platform for commercial buildings}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84991030390{\&}doi=10.1080{\%}2F23744731.2016.1218236{\&}partnerID=40{\&}md5=3c73d33417a9be3056a46942d3c3da9d},
