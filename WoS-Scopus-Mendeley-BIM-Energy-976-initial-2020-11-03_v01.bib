author = {El-Diraby, T and Krijnen, T and Papagelis, M},
doi = {10.1016/j.autcon.2017.06.004},
issn = {09265805},
journal = {Automation in Construction},
keywords = { Architectural design, Building Information Model - BIM; Collaboration framework; Green buildings; Industry Foundation Classes - IFC; Sociotechnical; Thermal simulations,Buildings; Design; Energy efficiency; Energy utilization; Information theory; Online systems; Semantics; Social networking (online)},
pages = {59--74},
publisher = {Elsevier B.V.},
title = {{BIM-based collaborative design and socio-technical analytics of green buildings}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85021368875{\&}doi=10.1016{\%}2Fj.autcon.2017.06.004{\&}partnerID=40{\&}md5=a4748dc0ac73da40f5241b3c3e8ea724},
