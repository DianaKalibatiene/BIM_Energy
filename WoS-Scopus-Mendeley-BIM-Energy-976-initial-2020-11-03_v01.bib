pages = {150--168},
publisher = {Department of Computer Science},
title = {{A bim-based web service framework for green building energy simulation and code checking}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84907376890{\&}partnerID=40{\&}md5=2f62f7e068a13717acf819b1e0948d62},
volume = {19},
