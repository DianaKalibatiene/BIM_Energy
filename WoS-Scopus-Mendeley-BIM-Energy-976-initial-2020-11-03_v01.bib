author = {Albano, R},
doi = {10.3390/app9214674},
issn = {20763417},
journal = {Applied Sciences (Switzerland)},
number = {21},
publisher = {MDPI AG},
title = {{Investigation on roof segmentation for 3D building reconstruction from aerial LIDAR point clouds}},
