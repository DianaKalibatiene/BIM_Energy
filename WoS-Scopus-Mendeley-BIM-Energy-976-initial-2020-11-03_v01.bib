pages = {1--17},
publisher = {Elsevier Ltd},
title = {{Application of BEM and using BIM database for BEM: A review}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85060290470{\&}doi=10.1016{\%}2Fj.jobe.2019.01.021{\&}partnerID=40{\&}md5=d2cf9b9f8745369485f1282b77a99649},
