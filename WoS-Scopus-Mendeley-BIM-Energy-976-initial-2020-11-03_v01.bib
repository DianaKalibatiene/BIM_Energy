keywords = { Architectural design, Building energy performance; Building Information Model - BIM; Energy simulation; Life cycle analysis; Literature reviews; Operational phase; Research patterns; Scientometric analysis,Energy efficiency; Information theory; Integration; Interoperability; Life cycle},
pages = {513--527},
publisher = {Elsevier Ltd},
title = {{A combined scientometric and conventional literature review to grasp the entire BIM knowledge and its integration with energy simulation}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85060279860{\&}doi=10.1016{\%}2Fj.jobe.2018.12.021{\&}partnerID=40{\&}md5=aabd6bfc2cf8d23a2a56c4d332e39cfc},
volume = {22},
year = {2019}
}
@article{Basack201922,
abstract = {The benefits of implementing Building Information Modelling (BIM) tools in architecture, engineering and construction (AEC) have gained importance with the higher level of interdependence amongst design and analysis tools used by architects, engineers and project managers. Energy analysis has become more relevant during the early design phase in the AEC industry as these calculations are usually carried out at the end of the design process and do not give room for a variety of alternatives. Energy analysis needs to be incorporated in BIM to create alternative design options by the architects thereby predicting the usage of energy during the operational phase of a building. An architect requires optimising the operational energy need of a building at early design stage with limited parameters available and a simplified approach. This study identified the gap in a few selected and existing simulation tools and proposed a prototype model with minimum input variation to aid architects in estimating and optimising operational cooling energy during the early design phase. For achieving this model, the calibration of utilisation factors based on the impact of orientation on wall, window and floor area in monthly quasi-steady-state (QSS) of operational energy calculation was adapted in composite climate in an Indian context. {\textcopyright} 2019 The Association of Building Engineers. All rights reserved.},
