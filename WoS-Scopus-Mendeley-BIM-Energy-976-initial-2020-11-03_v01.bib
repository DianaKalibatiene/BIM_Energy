doi = {10.1007/s12034-011-0076-6},
issn = {02504707},
journal = {Bulletin of Materials Science},
keywords = { Bacteria, Bacteriology; Biochemistry; Crystals; Indicators (chemical); Iron; Iron compounds; Iron ores; Iron oxides; Magnetite; Mineralogy; Nanoparticles; Paramagnetism; Redox reactions; Saturation magnetization,Alternative approach; Archaeoglobus fulgidus; Bacterial species; Biological methods; Biological route; Biotechnological applications; Bulk materials; Chemical method; Desulfovibrio; Environmental factors; Geobacter; Inorganic systems; Iron oxide nanoparticle; Iron reducing bacteria; Iron-based; Magnetite crystals; Magnetotactic Bacteria; Methods of synthesis; Microbes; Microbial synthesis; Neutral pH; Redox potentials; Shewanella; Size ranges; Sulphate reducing bacteria; Sulphates; Toxic chemicals},
