issn = {03787788},
journal = {Energy and Buildings},
keywords = { Architectural design, Building Information Model - BIM; Building performance; Parametric modeling; Performance based design; Visual programming,Algorithms; Buildings; Computer programming; Design; Energy efficiency; Energy utilization; Environmental management; Information theory; Multiobjective optimization; Open source software; Optimization; User interfaces},
pages = {401--412},
publisher = {Elsevier Ltd},
title = {{BPOpt: A framework for BIM-based performance optimization}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84944058350{\&}doi=10.1016{\%}2Fj.enbuild.2015.09.011{\&}partnerID=40{\&}md5=ecb17a409cdf71ed81f7739e15da48fe},
