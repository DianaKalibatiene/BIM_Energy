keywords = { Architectural design, Building Information Modelling; Comsol multiphysics; Energy; Fitness functions; Fundamental analysis; Heating and cooling; Heating appliances; Orthogonal array,Cooling; Energy dissipation; Location; Taguchi methods; Thermal effects},
number = {1},
