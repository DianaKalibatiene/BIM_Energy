keywords = {Building information modeling (BIM); Interoperability; Scientometric analysis; Scientometric mapping},
publisher = {ELSEVIER},
title = {{Interoperability in building information modeling for AECO/FM industry}},
type = {Article},
volume = {113},
year = {2020}
}
